{
	"name": "silver-memory",
	"description": "silver memory a bot for discord message proxying",
	"homepage": "https://github.com/bobbys-development-team/silver-memory/README.md",
	"eslintConfig": {
		"env": {
			"es6": true,
			"node": true
		},
		"extends": "eslint:recommended",
		"parserOptions": {
			"sourceType": "module",
			"ecmaVersion": 2020
		},
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"quotes": [
				"error",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"no-console": "off",
			"no-unused-vars": "off",
			"no-cond-assign": "off",
			"no-case-declarations": "off",
			"no-empty": "off"
		}
	},
	"dependencies": {
		"@sentry/node": "^6.5.1",
		"bufferutil": "^4.0.1",
		"dotenv": "^15.0.0",
		"eris": "github:keterr/eris#dev",
		"eris-sharder": "^1.10.0",
		"got": "^11.8.2",
		"ioredis": "^4.16.0",
		"pako": "^2.0.3",
		"pg": "^8.2.1",
		"probe-image-size": "^7.2.1",
		"string-length": "^5.0.0",
		"valid-url": "^1.0.9",
		"winston": "^3.2.1",
		"zlib-sync": "^0.1.6"
	},
	"devDependencies": {
		"eslint": "^7.10.0"
	}
}
